  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjnortier/react-redux-webpack-scaffolding/issues"
  },
  "homepage": "https://github.com/bjnortier/react-redux-webpack-scaffolding#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "glamor": "^2.20.40",
    "prop-types": "^15.6.0",
